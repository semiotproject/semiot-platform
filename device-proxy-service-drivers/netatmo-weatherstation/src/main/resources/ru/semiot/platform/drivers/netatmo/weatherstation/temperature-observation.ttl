@prefix hmtr: <http://purl.org/NET/ssnext/heatmeters#> .
@prefix meter: <http://purl.org/NET/ssnext/meters/core#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt#> .
@prefix qudt-quantity: <http://qudt.org/vocab/quantity#> .
@prefix : <${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}/observations/temperature#> .

[ a ssn:Observation ;
    ssn:observedProperty qudt-quantity:ThermodynamicTemperature ;
    ssn:observedBy <${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}-temperature> ;
    ssn:observationResultTime "${ru.semiot.platform.observation.datetime}"^^xsd:dateTime ;
    ssn:observationResult [ a ssn:SensorOutput ;
        ssn:isProducedBy <${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}-temperature> ;
        ssn:hasValue [ a qudt:QuantityValue ;
            qudt:quantityValue "${ru.semiot.drivers.netatmo.observation.value}"^^xsd:double ;
        ] ;
    ] ;
] .